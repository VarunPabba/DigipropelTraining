
using System;
class Leap_Year_Calculator
{
  static void Main()
  {
      Console.WriteLine("Enter Year : ");  //asking for the input from the use
      int Year = int.Parse(Console.ReadLine());   
      if (((Year % 4 == 0) && (Year % 100 != 0)) || (Year % 400 == 0)) Console.WriteLine("{0} is a Leap Year.", Year);  //giving every possible conditions for a leap year occured
      else Console.WriteLine("{0} is not a Leap Year.", Year);  //declaring if it not a leap year
  }
}
